package com.collections.map;

import java.sql.Timestamp;
import java.util.Date;

import javax.xml.crypto.Data;

public class EltpCalendar {
	public static void main(String args[]){
		Date date = new Date();
		Timestamp ts = new Timestamp(date.getTime());
		System.out.println(date.toString());
		System.out.println(ts.toString());
		System.out.println(date.toString()+ts.toString());
		System.out.println(" date occurs after "+date.after(ts));
		System.out.println("date occurs before "+date.before(ts));
		System.out.println("compare two dates "+date.compareTo(ts));
		System.out.println("timestramp of first date object "+date.toInstant().toString());
		System.out.println("change timestramp of first object"+date.);
		
	}
}
